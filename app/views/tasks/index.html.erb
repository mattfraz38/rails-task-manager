<h1>My Tasks</h1>
<ol>
   <% @tasks.each do |task| %>
    <li><%= link_to task.title, task_path(task) %> <%= link_to 'Delete', task_path(task), method: :delete, data: { confirm: "Are you sure you ant to delete?" } %></li>
     <p><%= task.completed %></p>
  <% end %>
</ol>
<%= link_to "Add a new task", new_task_path %>
